# -*- coding:utf-8 -*-
# class TreeNode:
#     def __init__(self, x):
#         self.val = x
#         self.left = None
#         self.right = None
class Solution:
    # 返回从上到下每个节点值列表，例：[1,2,3]
    def PrintFromTopToBottom(self, root):
        # write code here
        #一个队列
        que_list = []
        result = []
        if not root:
            return False

            #que_list.insert(0,root)
        que_list.append(root)
            #[object of type 'NoneType' has no len() 报错
            #'NoneType' object has no attribute 'val'
        while que_list:
            temp = que_list.pop(0)
                #为什么temp会为空？
            if temp:
                result.append(temp.val)
                if temp.left:
                        #que_list.insert(0,temp.left)
                    que_list.append(temp.left)
                if root.right:
                        #que_list.insert(0,temp.right)
                    que_list.append(temp.right)
        return result
        
 #为什么一直报错？
            
                
